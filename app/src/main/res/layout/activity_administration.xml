<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_margin="2dp"
    tools:context=".controlPanel.Administration">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/frameLayoutContainerAdmin"
        android:layout_above="@id/bottomNavAdmin"/>


    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/material_six"
        style="@style/Widget.Design.BottomNavigationView"
        android:theme="@style/Theme.AppCompat.DayNight.DarkActionBar"
        android:id="@+id/bottomNavAdmin"
        app:menu="@menu/bottom_nav_admin"
        android:layout_alignParentBottom="true"/>

</RelativeLayout>